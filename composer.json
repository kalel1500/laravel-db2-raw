{
    "name": "kalel1500/laravel-db2-raw",
    "description": "laravel-db2-raw is a simple helper to execute raw DB2 queries in Laravel.",
    "keywords": ["laravel", "package", "db2", "raw", "query", "helper"],
    "authors": [
        {
            "name": "Adrian Canals Csefko",
            "email": "a.canals14@gmail.com"
        }
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "ext-json": "*",
        "ext-ibm_db2": "*",
        "php" : "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Thehouseofel\\DB2Raw\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thehouseofel\\DB2Raw\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thehouseofel\\DB2Raw\\Db2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
